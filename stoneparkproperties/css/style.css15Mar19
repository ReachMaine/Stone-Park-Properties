.wpcf7-form textarea {
  height: 200px;
  background-color: white; }
.wpcf7-form input[type="text"],
.wpcf7-form input[type="password"],
.wpcf7-form input[type="datetime"],
.wpcf7-form input[type="datetime-local"],
.wpcf7-form input[type="date"],
.wpcf7-form input[type="month"],
.wpcf7-form input[type="time"],
.wpcf7-form input[type="week"],
.wpcf7-form input[type="number"],
.wpcf7-form input[type="email"],
.wpcf7-form input[type="url"],
.wpcf7-form input[type="search"],
.wpcf7-form input[type="tel"],
.wpcf7-form input[type="color"] {
  max-width: 100%;
  height: 2em;
  margin-bottom: 10px; }
.wpcf7-form input[type="tel"] {
  width: 200px; }
.wpcf7-form input.cf7-med {
  width: 200px; }
.wpcf7-form input.cf7-small {
  width: 80px; }
.wpcf7-form input.wpcf7-form-control.wpcf7-submit {
  padding: 6px 14px;
  border: 1px solid white;
  border-radius: 3px;
  text-transform: uppercase;
  font-weight: bold;
  color: white;
  background-color: #666666; }
.wpcf7-form input.wpcf7-form-control.wpcf7-submit:hover {
  background-color: #cbb677; }
.wpcf7-form p {
  margin-bottom: 5px; }
.wpcf7-form label.cf7-required:after {
  content: "*";
  color: red; }
.wpcf7-form .one-half, .wpcf7-form .one-third, .wpcf7-form .two-third, .wpcf7-form .one-fifth {
  position: relative;
  padding-right: 2%;
  padding-bottom: 5px; }
.wpcf7-form .one-half {
  width: 45%; }
.wpcf7-form .one-third {
  width: 30%; }
.wpcf7-form .two-third {
  width: 60%; }
.wpcf7-form .one-fifth {
  width: 20%;
  min-width: 80px; }
.wpcf7-form .one-half.last, .wpcf7-form .one-third.last, .wpcf7-form .two-third.last {
  padding-right: 0px;
  clear: right; }
.wpcf7-form .wpcf7-not-valid-tip {
  color: #c02646; }

.wpcf7 .wpcf7-form .wpcf7-response-output {
  background-color: white;
  font-weight: bold; }

.wpcf7 .wpcf7-form .wpcf7-response-output:not(.wpcf7-mail-sent-ok) {
  border-color: #c02646;
  color: #c02646; }

.wpcf7 .wpcf7-form .wpcf7-response-output.wpcf7-mail-sent-ok {
  color: #666666;
  background-color: white;
  font-weight: bold;
  border-color: #cbb677; }

#top-menu li {
  padding-right: 30px; }
  #top-menu li ul.sub-menu li {
    line-height: 20px; }
    #top-menu li ul.sub-menu li a {
      padding-top: 10px;
      padding-bottom: 10px; }
  #top-menu li ul.sub-menu li.spp-menu-header {
    padding-left: 0px;
    font-weight: bold; }
  #top-menu li ul.sub-menu li:not(.current-menu-item) > a {
    color: #a9a9a9; }
  #top-menu li ul.sub-menu li:hover a {
    background-color: white; }
  #top-menu li ul.sub-menu li > a:hover {
    background-color: white;
    font-weight: bold; }
  #top-menu li ul.sub-menu li:not(.current-menu-item) > a:hover {
    color: #1d1d1d !important; }

